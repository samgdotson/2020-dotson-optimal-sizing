	file = {Turrin et al. - 2011 - Design explorations of performance driven geometry.pdf:/home/dotson/Zotero/storage/ZKKVNS2U/Turrin et al. - 2011 - Design explorations of performance driven geometry.pdf:application/pdf}
}

