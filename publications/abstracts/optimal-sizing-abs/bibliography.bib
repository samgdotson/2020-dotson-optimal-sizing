	langid = {english},
	keywords = {Air pollution, Climate change, Hydrogen energy, Hydrogen generation, Solar-hydrogen},
	file = {ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/F6T622Y4/Nowotny and Veziroglu - 2011 - Impact of hydrogen on the environment.pdf:application/pdf;ScienceDirect Snapshot:/home/dotson/Zotero/storage/AX7RMM8B/S0360319911017319.html:text/html}
}

@article{shaner_comparative_2016,
	title = {A comparative technoeconomic analysis of renewable hydrogen production using solar energy},
	volume = {9},
	issn = {1754-5706},
	url = {https://pubs.rsc.org/en/content/articlelanding/2016/ee/c5ee02573g},
