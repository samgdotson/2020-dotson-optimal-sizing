@article{mourogov_potentialities_2006,
	title = {Potentialities of the fast spectrum molten salt reactor concept: {REBUS}-3700},
	volume = {47},
	issn = {0196-8904},
	url = {http://www.sciencedirect.com/science/article/pii/S0196890406000549},
	doi = {10.1016/j.enconman.2006.02.013},
	series = {12th International Conference on Emerging Nuclear Energy Systems},
	shorttitle = {Potentialities of the fast spectrum molten salt reactor concept},
	abstract = {The work is realized in the frame of {EDF} R\&D project devoted to innovative nuclear systems. This paper deals with the characteristics of fast reactor {REBUS}-3700 using molten salt fuel consisting of uranium and transuranics trichlorides dissolved in sodium chloride. A description of the system design, as well as a justification of its operating parameters is given. The performed neutronics studies demonstrate that {REBUS}-3700 gathers a positive breeding gain and strong negative salt expansion reactivity feedback. The safety analysis demonstrates an excellent reactor behavior with respect to the majority of single and combined unprotected events. The work also reveals the necessity to get more accurate nuclear data for the chlorine isotopes and to investigate the reactor configuration with radial fertile blanket. It seems also indispensable to realize more detailed thermo-hydraulic calculations and especially to perform a sizeable R\&D program on structure material capable to resist the corrosion induced by chlorides.},
	pages = {2761--2771},
	number = {17},
	journaltitle = {Energy Conversion and Management},
	shortjournal = {Energy Conversion and Management},
	author = {Mourogov, A. and Bokov, P. M.},
	urldate = {2018-06-11},
	date = {2006-10-01},
	keywords = {Fast reactor, Molten salt fuel, Safety analysis},
	file = {ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/C7BX7H3V/Mourogov and Bokov - 2006 - Potentialities of the fast spectrum molten salt re.pdf:application/pdf;ScienceDirect Snapshot:/home/dotson/Zotero/storage/KYQZCVB7/S0196890406000549.html:text/html;ScienceDirect Snapshot:/home/dotson/Zotero/storage/G7KDK8B6/S0196890406000549.html:text/html}
