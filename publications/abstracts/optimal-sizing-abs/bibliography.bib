@inproceedings{romano_novel_2001,
	title = {{NOVEL} {FUEL} {GEOMETRIES} {FOR} {THE} {GENERATION} {IV} "{IRIS}" {REACTOR}},
	url = {https://www.osti.gov/etdeweb/servlets/purl/20235334},
	abstract = {Cylindrical wire wrapped fuel pins are the primary design choice for the tightly packed fuel
array of the epithermal core option of the {IRIS} reactor. However it is desirable to
investigate alternative approaches to enhance thermal margins. Hence novel nuclear fuel
shapes have been studied for use as backup configurations in the {IRIS} reactor. The
performance of these shapes compared with cylindrical fuel pins has been assessed by
introduction of figures of merit (performance indices P/s and 77s) capable of relating
geometrical properties to key thermalhydraulic parameters such as core pressure drop,
critical heat flux, fuel centerline temperature, clad surface temperature and stress
distribution. The performance indices have been determined for various arrangements of
cylindrical, triangular, square and hexagonal fuel shapes. Among these fuel candidates,
the hexagonal fuel pins, both twisted and straight, proved to be the best backup solution
for adoption by {IRIS}. The advantages of the hexagonal shape versus the traditional
cylindrical shape with grid spacers are several. The twisted hex allows (a) a reduction of
the total pressure drop by 16\%, (b) an increase of the critical heat flux ratio margin by
200\%. The straight hex allows flatter subchannel velocity profile, wall shear stress and
wall temperature distributions. The latter result, that is due to a uniform subchannel
section width, has come from a {CFD} analysis carried out to confirm the results of the
performance parameter approach.},
	author = {Romano, A and {KRIVENTSEV}, V and Ninokata, H. and Todreas, N.},
	urldate = {2019-11-11},
