@article{lenzen_life_2008,
	title = {Life cycle energy and greenhouse gas emissions of nuclear energy: A review},
	volume = {49},
	issn = {0196-8904},
	url = {http://www.sciencedirect.com/science/article/pii/S0196890408000575},
	doi = {10.1016/j.enconman.2008.01.033},
	shorttitle = {Life cycle energy and greenhouse gas emissions of nuclear energy},
	abstract = {The increased urgency of dealing with mitigation of the looming climate change has sparked renewed interest in the nuclear energy option. There exists a substantial stream of research on the amount of embodied energy and greenhouse gas emissions associated with nuclear generated electricity. While conventional fossil fuelled power plants cause emissions almost exclusively from the plant site, the majority of greenhouse gas emissions in the nuclear fuel cycle are caused in processing stages upstream and downstream from the plant. This paper distils the findings from a comprehensive literature review of energy and greenhouse gas emissions in the nuclear fuel cycle and determines some of the causes for the widely varying results. The most popular reactor types, {LWR} and {HWR}, need between 0.1 and 0.3kWhth, and on average about 0.2kWhth for every {kWh} of electricity generated. These energy intensities translate into greenhouse gas intensities for {LWR} and {HWR} of between 10 and 130g {CO}2-e/{kWhel}, with an average of 65g {CO}2-e/{kWhel}. While these greenhouse gases are expectedly lower than those of fossil technologies (typically 600–1200g {CO}2-e/{kWhel}), they are higher than reported figures for wind turbines and hydroelectricity (around 15–25g {CO}2-e/{kWhel}) and in the order of, or slightly lower than, solar photovoltaic or solar thermal power (around 90g {CO}2-e/{kWhel}).},
	pages = {2178--2199},
	number = {8},
	journaltitle = {Energy Conversion and Management},
	shortjournal = {Energy Conversion and Management},
	author = {Lenzen, Manfred},
	urldate = {2018-06-15},
	date = {2008-08-01},
	keywords = {Fuel cycle, Nuclear energy, Greenhouse gas emissions, Life cycle assessment, Embodied energy},
	file = {ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/XAA27XV5/Lenzen - 2008 - Life cycle energy and greenhouse gas emissions of .pdf:application/pdf;ScienceDirect Snapshot:/home/dotson/Zotero/storage/SH66BMML/S0196890408000575.html:text/html}
}

@article{sims_carbon_2003,
