	editoratype = {collaborator},
	urldate = {2018-09-27},
	date = {2018-04},
	file = {Fulltext:/home/dotson/Zotero/storage/SBUDQM3I/Rykhlevskii - 2018 - Advanced online fuel reprocessing simulation for t.pdf:application/pdf;ms_rykhlevskii_msbr_reproc.pdf:/home/dotson/Zotero/storage/EI334QNX/ms_rykhlevskii_msbr_reproc.pdf:application/pdf;Snapshot:/home/dotson/Zotero/storage/LWTBSFX7/101052.html:text/html}
