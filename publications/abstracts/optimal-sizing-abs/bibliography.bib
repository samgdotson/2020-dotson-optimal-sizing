	langid = {english},
	keywords = {{FLiBe}, Molten Salt Reactor Experiment ({MSRE}), Molten Salt Reactors ({MSR}), System Analysis Module ({SAM})},
	file = {ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/D7J944GN/Leandro et al. - 2019 - Thermal hydraulic model of the molten salt reactor.pdf:application/pdf;ScienceDirect Snapshot:/home/dotson/Zotero/storage/EWE9JHXI/S0306454918305887.html:text/html}
