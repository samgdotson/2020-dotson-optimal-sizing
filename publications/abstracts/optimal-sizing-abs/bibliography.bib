@article{wang_analysis_2017,
	title = {Analysis of {BEAVRS} two-cycle benchmark using {RMC} based on full core detailed model},
	issn = {0149-1970},
	url = {http://www.sciencedirect.com/science/article/pii/S0149197017300847},
	doi = {10.1016/j.pnucene.2017.04.009},
	abstract = {With the increasing demands of high fidelity neutronics analysis and the development of computer technology, Monte Carlo method is becoming more and more attractive especially in criticality analysis of initial core and shielding calculations, due to its advantages of flexible geometry modeling and use of continuous-energy nuclear cross sections. However, nuclear reactors are complex systems with different physics and feedback interactions and coupling. To perform the high fidelity multi-physics simulations of real reactors or benchmark calculations such as two-cycle {BEAVRS} benchmark based on measurement data of a practical nuclear power plant, several factors must be considered such as large scale detailed depletion, thermal-hydraulics feedback, on-the-fly nuclear cross section processing, criticality search and inter-cycle refueling. In this paper, the abilities mentioned above for multiple burnup cycles simulations in Hot Full Power condition of {PWR} full core have been developed in continuous-energy Reactor Monte Carlo neutron and photon transport code {RMC}, which is developed by Department of Engineering Physics at Tsinghua University, Beijing (Wang et al., 2015). {RMC} has the capacity for lifecycle simulations of nuclear reactor cores. The {BEAVRS} benchmark was selected as an example and {RMC} was applied to a full core, two cycle burnup calculation of {BEAVRS}. All of the parameters given in the {BEAVRS} benchmark have been calculated and compared with the measured values of {BEAVRS} benchmark. For other parameters such as pin power distributions, they are compared to the results of other codes. The results of {RMC} agree well with the measured values of {BEAVRS} benchmark and also agree well with those of other codes. This was the first time for a Monte Carlo code to perform the full core, two cycle calculation of {BEAVRS}. This work paves the way for Monte Carlo codes in life cycle simulations of nuclear reactor cores.},
	journaltitle = {Progress in Nuclear Energy},
	shortjournal = {Progress in Nuclear Energy},
	author = {Wang, Kan and Liu, Shichang and Li, Zeguang and Wang, Gang and Liang, Jingang and Yang, Feng and Chen, Zonghuan and Guo, Xiaoyu and Qiu, Yishu and Wu, Qu and Guo, {JuanJuan} and Tang, Xiao},
	urldate = {2017-04-18},
	date = {2017},
	keywords = {refueling, {BEAVRS}, Critical search, Full-core detailed burnup calculation, Neutronics/thermal-hydraulics coupling, {RMC}},
	file = {ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/R94BT9WM/Wang et al. - Analysis of BEAVRS two-cycle benchmark using RMC b.pdf:application/pdf;ScienceDirect Snapshot:/home/dotson/Zotero/storage/XK9QCF9V/S0149197017300847.html:text/html}
}

@report{burion_benchmarks_2019,
	location = {Paris, France},
	title = {Benchmarks for Uncertainty Analysis in Modeling ({UAM}) for Design, Operation, and Safety Analysis of {SFRs}},
	url = {https://www.oecd-nea.org/download/sfr-uam/index.html},
	shorttitle = {{AEN}-{WPRS} {SFR}-{UAM}},
	abstract = {A series of benchmarks on which the different participants could compare their abilities to
calculate the nominal performance characteristics and the global safety parameters of Sodiumcooled Fast Reactor ({SFR}) cores was launched within the Working Party on Reactor and System
({WPRS}) under the {SFR}-{FT} task force. The work concentrated on two different core sizes: 2
large size cores (3600 {MW} thermal) and 2 medium size cores (1000 {MW} thermal), and three
types of fuel: oxide, carbide and metal. This comparative study focused on the following items:
 Neutronic characterization of global parameters (k-effective, power distributions, sodium
void worth, Doppler, etc.)
 Feedback coefficient evaluation, discussion and agreement on corresponding calculation
methodology.
This work is summarized in the final report [1] and confirmed the ability of participants and their
neutronic codes to provide generally consistent results when analyzing {SFR} core characteristics.
Some remaining inconsistencies with the results of some participants could not be investigated
and still need to be understood. In particular, discrepancies between “best estimate”
deterministic and Monte-Carlo results need to be investigated in depth in order to identify
remaining bias. Beyond this important task, the detailed review of modeling effects that was
performed in this first benchmark paved the way of important recommendations on general
neutronic schemes to be adopted for accurate reactivity effect evaluation using deterministic
codes:
 Heterogeneous spatial description of subassembly for cross section generation
 Fine group (few thousand group) energy description for self-shielding effects
 Specific treatment in order to take into account spatial self-shielding effect on control rod
cross section
The initial objectives of the {SFR}-{FT} benchmark also involved calculating the feedback
coefficients and simulating unprotected transients, but those could not be reached within this
initial framework. This is why a follow-on benchmark exercise hosted within the {UAM} group is
proposed in order to conduct the calculation of neutronic feedback coefficients and their
uncertainties at the End of Cycle ({EOC}). This benchmark proposal also includes in a second step
the analyses of unprotected transient. Two {SFR} cores among the 4 being studied in the {SFR}-{FT}
group were selected and are described in this document. Those are the large oxide core proposed
by {CEA} and the medium metallic core proposed by {ANL}. Since the remaining inconsistencies of
the previous benchmark were concentrated on the burnup calculations, the cores descriptions
proposed in this new benchmark are directly provided at the end of cycle in order to minimize
the sources of discrepancies dues to the different depletion codes used by the participants.
The neutronic feedback coefficients and their uncertainties at the end of cycle will be conducted
within the first step of this benchmark using a common calculation methodology detailed in this
document. In particular the axial and radial thermal expansion coefficients that were missing in
the previous benchmark exercise will be estimated. 
Based on the results obtained in the previous step, transient calculations will be performed on a
few selected cases for the principal unprotected transients: unprotected transient overpower
({UTOP}), unprotected loss of flow ({ULOF}), Unprotected Control Rod Withdrawal ({UCRW}) and
possibly in a later stage unprotected loss of heat sink ({ULOHS}).},
	pages = {1--58},
	number = {Version 1.6},
	institution = {{OECD} Nuclear Energy Agency},
	type = {Working Party on Reactor and System},
	author = {Burion, Laurent and Rimpault, Gerald and Sciora, Pierre and Stauff, Nicolas and Kim, Taek Kym and Taiwo, Temitope and Boastelmann, Friederike and Pautz, Andreas and Ivanov, Kostadin and Trivedi, Ishita and Kereszturi, Andras and Aures, Alexander and Velkov, Kiril and Zwermann, Winfried and Mikityuk, Konstantin},
	urldate = {2019-05-03},
	date = {2019-05-02},
	note = {https://www.oecd-nea.org/download/sfr-uam/index.html},
	file = {Burion et al. - 2019 - Benchmarks for Uncertainty Analysis in Modeling (U.pdf:/home/dotson/Zotero/storage/DHEWESXI/Burion et al. - 2019 - Benchmarks for Uncertainty Analysis in Modeling (U.pdf:application/pdf}
}

