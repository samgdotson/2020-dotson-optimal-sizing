}

@article{forsberg_fluoride-salt-cooled_2019,
	title = {Fluoride-Salt-Cooled High-Temperature Reactor ({FHR}) Using British Advanced Gas-Cooled Reactor ({AGR}) Refueling Technology and Decay Heat Removal Systems That Prevent Salt Freezing},
	volume = {205},
	issn = {0029-5450, 1943-7471},
	url = {https://www.tandfonline.com/doi/full/10.1080/00295450.2019.1586372},
	doi = {10.1080/00295450.2019.1586372},
	abstract = {The flouride-salt-cooled high-temperature reactor ({FHR}) uses graphite-matrix coated-particle fuel [the same as high-temperature gas-cooled reactors ({HTGRs})] and a clean liquid salt coolant. It delivers heat to the industrial process or the power cycle at temperatures between 600°C and 700°C with average heat delivery temperatures higher than for other reactors. The melting point of the liquid salt coolant is above 450°C. The high minimum temperatures present refueling challenges and require special features to control temperatures, avoiding excessively high temperatures and freezing of the coolant that could impact decay heat cooling systems. This paper describes a preconceptual {FHR} design that addresses many of these challenges by adopting features from the British advanced gas-cooled reactor ({AGR}) and alternative decay heat cooling systems. The bases for specific design choices are described.},
