@article{komiyama_long-term_2015,
	title = {Long-term scenario analysis of nuclear energy and variable renewables in Japan's power generation mix considering flexible power resources},
	volume = {83},
	issn = {0301-4215},
	url = {http://www.sciencedirect.com/science/article/pii/S0301421515001470},
	doi = {10.1016/j.enpol.2015.04.005},
	abstract = {This paper comprehensively analyzes an optimal deployment of variable renewables ({VRs}) with flexible power resources, such as electricity saving and rechargeable battery, in Japan's long-term power generation mix to 2050 under possible nuclear energy scenarios. The study is performed, employing a dynamic high time-resolution optimal power generation mix model which is formulated as a large-scale linear programming model. Simulation results show that both complete nuclear phase-out and carbon reduction by 80\% in 2050 from 2010 encourage {VR} expansion in the country's power system and cause a quadruple increase of power generation cost at 2050 compared with that under current nuclear capacity and no carbon regulation policy; long-term cost reduction of {VR} energy system is necessary if {VR} is positioned as a mainstream for future sustainable power supply. Secondly, higher levels of {VR} integration decrease the capacity factor of {LNG} combined cycle ({LNGCC}), which implies the challenge to assure {LNGCC} serving as a remunerated ramp generator for {VR} intermittency. Finally, as an economically optimal solution, electricity saving serves as an important option to integrate massive {VR} and to treat a seasonal imbalance of its power output in an efficient way.},
	pages = {169--184},
	journaltitle = {Energy Policy},
	shortjournal = {Energy Policy},
	author = {Komiyama, Ryoichi and Fujii, Yasumasa},
	urldate = {2018-01-30},
	date = {2015-08-01},
	keywords = {Nuclear, Rechargeable battery, Variable renewable, Power generation mix},
	file = {Fulltext:/home/dotson/Zotero/storage/CPZG4K49/S0301421515001470.html:text/html;ScienceDirect Snapshot:/home/dotson/Zotero/storage/WCH2FLSL/S0301421515001470.html:text/html;Snapshot:/home/dotson/Zotero/storage/PMCPFJNH/S0301421515001470.html:text/html}
}

@article{komiyama_assessment_2014,
	title = {Assessment of massive integration of photovoltaic system considering rechargeable battery in Japan with high time-resolution optimal power generation mix model},
	volume = {66},
	issn = {0301-4215},
	url = {http://www.sciencedirect.com/science/article/pii/S0301421513011294},
	doi = {10.1016/j.enpol.2013.11.022},
	abstract = {Maximizing renewables in the country's power system has been a key political agenda in Japan after the Fukushima nuclear disaster. This paper investigates the potential of {PV} resource, which could be systematically integrated into the Japanese power system, using a high time-resolution optimal power generation mix model. The model allows us to explicitly consider actual {PV} and wind output variability in 10-min time resolution for 365 days. Simulation results show that, as {PV} expands, the growth of {PV} integration into the grid slows down when the installed {PV} capacity is more than the scale of the peak demand, although Japan has immense potential of installable {PV} capacity – equivalent to 40 times of the peak. Secondly, the results imply that a large-scale {PV} integration potentially decreases the usage ratio of {LNG} combined cycle ({LNGCC}) in specific seasons, which is a challenge for utility companies to ensure that {LNGCC} is used as a profitable compensating generator for {PV} variability. Finally, a sensitivity analysis on rechargeable battery cost suggests that the reason for suppressing the {PV} output instead of storing its surplus output by the battery can be attributed to the high battery cost; hence, the improvement of its economic performance is significant to integrate the massive {PV} energy.},
	pages = {73--89},
	journaltitle = {Energy Policy},
	shortjournal = {Energy Policy},
	author = {Komiyama, Ryoichi and Fujii, Yasumasa},
	urldate = {2018-01-30},
	date = {2014-03-01},
	keywords = {Optimal power generation mix, Rechargeable battery, {PV} system},
	file = {ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/5M9GD3II/Komiyama and Fujii - 2014 - Assessment of massive integration of photovoltaic .pdf:application/pdf;ScienceDirect Snapshot:/home/dotson/Zotero/storage/THQU4BS5/S0301421513011294.html:text/html;ScienceDirect Snapshot:/home/dotson/Zotero/storage/7E7T3NAX/S0301421513011294.html:text/html}
}

