@unpublished{dolan_molten_2018,
	location = {Urbana, {IL}},
	title = {Molten Salt Reactors and Thorium Energy},
	url = {https://emails.illinois.edu/newsletter/182700.html},
	abstract = {The simplest molten salt reactor could use solid fuel, as in a conventional reactor, but with molten salt coolant replacing the water, helium, or liquid sodium. An {MSR} could operate at low pressure, avoiding the dangers of evaporation, steam pressure, and hydrogen explosion. The solid fuel could also be replaced by molten salt fuels, such as {UF}4. In a liquid fuel {MSR} the fuel content could be processed continuously to remove fission products, adjust reactivity, and incinerate actinide wastes. The reactor could include {ThF}4 to breed U-233. Most of the thorium could be converted into fissile U-233, but it is more difficult to breed fissile Pu-239 from U-238, and thorium is four times as abundant as uranium in the earth’s crust. The low pressure of {MSRs} allows lighter weight, less expensive vessels. {MSRs} could operate safely at nearly atmospheric pressure and high temperature, yielding efficient electrical power generation, desalination, actinide incineration, and hydrogen production.},
