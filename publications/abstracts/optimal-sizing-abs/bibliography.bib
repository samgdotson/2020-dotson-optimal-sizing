	langid = {english},
	file = {Full Text PDF:/home/dotson/Zotero/storage/4RKUE8Z5/Song et al. - 2014 - Pore Scale Thermal Hydraulics Investigations of Mo.pdf:application/pdf;Snapshot:/home/dotson/Zotero/storage/WFG4PHJ5/ref.html:application/xhtml+xml}
}

