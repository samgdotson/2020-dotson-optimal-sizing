}

@article{li_effects_2018,
	title = {Effects of fuel salt composition on fuel salt temperature coefficient ({FSTC}) for an under-moderated molten salt reactor ({MSR})},
	volume = {29},
	issn = {1001-8042, 2210-3147},
	url = {https://link.springer.com/article/10.1007/s41365-018-0458-1},
	doi = {10.1007/s41365-018-0458-1},
	abstract = {With respect to a liquid-fueled molten salt reactor ({MSR}), the temperature coefficient of reactivity mainly includes the moderator temperature coefficient ({MTC}) and the fuel salt temperature coefficient ({FSTC}). The {FSTC} is typically divided into the Doppler coefficient and the density coefficient. In order to compensate for the potentially positive {MTC}, the {FSTC} should be sufficiently negative, and this is mostly optimized in terms of the geometry aspect in pioneering studies. However, the properties of fuel salt also directly influence the {FSTC}. Thus, the effects of different fuel salt compositions including the 235235{\textasciicircum}\{235\}U enrichment, heavy metal proportion in salt phase ({HM} proportion), and the 77{\textasciicircum}\{7\}Li enrichment on {FSTC} are investigated from the viewpoint of the essential six-factor formula. The analysis is based on an under-moderated {MSR}. With respect to the Doppler coefficient, the temperature coefficient of the fast fission factors (αT(ε)αT(ε){\textbackslash}alpha \_\{{\textbackslash}text \{T\}\}({\textbackslash}varepsilon )) is positive and those of the resonance escape probability (αT(p)αT(p){\textbackslash}alpha \_\{{\textbackslash}text \{T\}\}(p )), thermal reproduction factor (αT(η)αT(η){\textbackslash}alpha \_\{{\textbackslash}text \{T\}\}({\textbackslash}eta )), thermal utilization factor (αT(f)αT(f){\textbackslash}alpha \_\{{\textbackslash}text \{T\}\}(f )), and total non-leakage probability (αT(Λ)αT(Λ){\textbackslash}alpha \_\{{\textbackslash}text \{T\}\}({\textbackslash}{varLambda} )) are negative. With respect to the density coefficient, αT(p)αT(p){\textbackslash}alpha \_\{{\textbackslash}text \{T\}\}(p ) and αT(η)αT(η){\textbackslash}alpha \_\{{\textbackslash}text \{T\}\}({\textbackslash}eta ) are positive, while the others are negative. The results indicate that the effects of the 235235{\textasciicircum}\{235\}U enrichment and {HM} on {FSTC} are mainly reflected in αT(ε)αT(ε){\textbackslash}alpha \_\{{\textbackslash}text \{T\}\}({\textbackslash}varepsilon ) and αT(p)αT(p){\textbackslash}alpha \_\{{\textbackslash}text \{T\}\}(p ), which are the dominant factors when the neutron spectrum is relatively hard. Furthermore, the 77{\textasciicircum}\{7\}Li enrichment influences {FSTC} by αT(f)αT(f){\textbackslash}alpha \_\{{\textbackslash}text \{T\}\}(f ) and αT(Λ)αT(Λ){\textbackslash}alpha \_\{{\textbackslash}text \{T\}\}({\textbackslash}{varLambda} ), which are the key factors in a relative soft spectrum. In order to obtain a more negative {FSTC} for an under-moderated {MSR}, the possible positive density coefficient, especially its αT(p)αT(p){\textbackslash}alpha \_\{{\textbackslash}text \{T\}\}(p ), should be suppressed. Thus, a lower 235235{\textasciicircum}\{235\}U enrichment (albeit higher than a certain value, 5 wt\% in this article) along with a lower {HM} proportion and/or a higher 77{\textasciicircum}\{7\}Li enrichment are recommended. The analyses provide an approach to achieve a more suitable fuel salt composition with a sufficiently negative {FSTC}.},
	pages = {110},
	number = {8},
	journaltitle = {Nuclear Science and Techniques},
	shortjournal = {{NUCL} {SCI} {TECH}},
	author = {Li, Xiao-Xiao and Ma, Yu-Wen and Yu, Cheng-Gang and Zou, Chun-Yan and Cai, Xiang-Zhou and Chen, Jin-Gen},
	urldate = {2018-07-10},
