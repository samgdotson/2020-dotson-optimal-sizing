	keywords = {{PWR}, Accident tolerant cladding, {CHF}, {RIA}, Wettability},
	file = {ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/VZV4TJFP/Liu et al. - 2017 - Potential impact of accident tolerant fuel claddin.pdf:application/pdf;ScienceDirect Snapshot:/home/dotson/Zotero/storage/KWDM7MU5/S0306454917300671.html:text/html}
}

@article{kulikov_physical_2016,
	title = {Physical particularities of nuclear reactors using heavy moderators of neutrons},
	volume = {79},
	issn = {1063-7788, 1562-692X},
	url = {https://link.springer.com/article/10.1134/S1063778816090076},
	doi = {10.1134/S1063778816090076},
	abstract = {In nuclear reactors, thermal neutron spectra are formed using moderators with small atomic weights. For fast reactors, inserting such moderators in the core may create problems since they efficiently decelerate the neutrons. In order to form an intermediate neutron spectrum, it is preferable to employ neutron moderators with sufficiently large atomic weights, using 233U as a fissile nuclide and 232Th and 231Pa as fertile ones. The aim of the work is to investigate the properties of heavy neutron moderators and to assess their advantages. The analysis employs the {JENDL}-4.0 nuclear data library and the {SCALE} program package for simulating the variation of fuel composition caused by irradiation in the reactor. The following main results are obtained. By using heavy moderators with small neutron moderation steps, one is able to (1) increase the rate of resonance capture, so that the amount of fertile material in the fuel may be reduced while maintaining the breeding factor of the core; (2) use the vacant space for improving the fuel-element properties by adding inert, strong, and thermally conductive materials and by implementing dispersive fuel elements in which the fissile material is self-replenished and neutron multiplication remains stable during the process of fuel burnup; and (3) employ mixtures of different fertile materials with resonance capture cross sections in order to increase the resonance-lattice density and the probability of resonance neutron capture leading to formation of fissile material. The general conclusion is that, by forming an intermediate neutron spectrum with heavy neutron moderators, one can use the fuel more efficiently and improve nuclear safety.},
	pages = {1429--1439},
	number = {9},
	journaltitle = {Physics of Atomic Nuclei},
	shortjournal = {Phys. Atom. Nuclei},
	author = {Kulikov, G. G. and Shmelev, A. N.},
	urldate = {2017-08-03},
	date = {2016-12-01},
	langid = {english},
	file = {Full Text PDF:/home/dotson/Zotero/storage/CJ9VHWBF/Kulikov and Shmelev - 2016 - Physical particularities of nuclear reactors using.pdf:application/pdf;Snapshot:/home/dotson/Zotero/storage/QX7WFCXR/S1063778816090076.html:text/html}
}

@article{ruan_specialized_2017,
	title = {A specialized code for operation transient analysis and its application in fluoride salt-cooled high-temperature reactors},
	volume = {28},
	issn = {1001-8042, 2210-3147},
	url = {https://link.springer.com/article/10.1007/s41365-017-0268-x},
	doi = {10.1007/s41365-017-0268-x},
	abstract = {Fluoride salt-cooled high-temperature reactors ({FHRs}) include many attractive features, such as high temperature, large heat capacity, low pressure and strong inherent safety. Transient characteristics of {FHR} are particularly important for evaluating its operation performance. Thus, a specialized code {OCFHR} (operation and control analysis code of {FHR}) issued to study an experimental {FHR}’s operation behaviors. The geometric modeling of {OCFHR} is based on one-dimensional lumped parameter method, and some simplifications are taken into consideration during simulation due to the existence of complex structures such as pebble bed, intermediate heat exchanger ({IHX}), air radiator ({AR}) and multiply channels. A point neutron kinetics model is developed, and neutron physics calculation is needed to provide some key inputs including axial power density distribution, reactivity coefficients and parameters about delayed neutron precursors. For analyzing the operational performance, five disturbed transients are simulated, involving reactivity step insertion, variations of coolant mass flow rate of primary loop and intermediate loop, adjustment of air inlet temperature and mass flow rate of air cooling system. Simulation results indicate that inherent self-stability of {FHR} restrains severe consequences under above transients, and some dynamic features are observed, such as large negative temperature feedbacks, remarkable thermal inertia and high response delay.},
	pages = {119},
	number = {8},
	journaltitle = {Nuclear Science and Techniques},
	shortjournal = {{NUCL} {SCI} {TECH}},
	author = {Ruan, Jian and Xu, Bo and Li, Ming-Hai and Yang, Yang and Zou, Yang and Xu, Hong-Jie},
	urldate = {2017-08-03},
	date = {2017-08-01},
	langid = {english},
	file = {Full Text PDF:/home/dotson/Zotero/storage/73WV29DJ/Ruan et al. - 2017 - A specialized code for operation transient analysi.pdf:application/pdf;Snapshot:/home/dotson/Zotero/storage/B7KJQ6N4/s41365-017-0268-x.html:text/html}
}

