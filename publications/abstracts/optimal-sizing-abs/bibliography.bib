	journaltitle = {Nuclear Engineering and Design},
	shortjournal = {Nuclear Engineering and Design},
	author = {Merzari, E. and Fischer, P. and Yuan, H. and Van Tichelen, K. and Keijers, S. and De Ridder, J. and Degroote, J. and Vierendeels, J. and Doolaard, H. and Gopala, V. R. and Roelofs, F.},
	urldate = {2016-09-27},
	date = {2016-03-01},
	file = {Fulltext:/home/dotson/Zotero/storage/LAZRYXGR/Merzari et al. - 2016 - Benchmark exercise for fluid flow simulations in a.pdf:application/pdf;ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/ZF8X62H5/Merzari et al. - 2016 - Benchmark exercise for fluid flow simulations in a.pdf:application/pdf;ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/KAATK9SF/Merzari et al. - 2016 - Benchmark exercise for fluid flow simulations in a.pdf:application/pdf;ScienceDirect Snapshot:/home/dotson/Zotero/storage/KJMB4M4B/S002954931500494X.html:text/html;ScienceDirect Snapshot:/home/dotson/Zotero/storage/N4FAAATR/S002954931500494X.html:text/html;Snapshot:/home/dotson/Zotero/storage/7CTC2NLL/S002954931500494X.html:text/html}
