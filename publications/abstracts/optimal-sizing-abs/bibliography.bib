	keywords = {{CO} emissions, Energy consumption, {STELLA}, System dynamics model},
	file = {ScienceDirect Snapshot:/home/dotson/Zotero/storage/JIF5B3H2/S0304380012004735.html:text/html}
}

@article{li_energy_2010,
	title = {Energy demand and carbon emissions under different development scenarios for Shanghai, China},
