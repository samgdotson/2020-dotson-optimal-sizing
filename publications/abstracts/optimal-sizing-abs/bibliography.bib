@article{laureau_transient_2015,
	title = {Transient Fission Matrix: Kinetic calculation and kinetic parameters βeff and Λeff calculation},
	volume = {85},
	issn = {0306-4549},
	url = {http://www.sciencedirect.com/science/article/pii/S0306454915003825},
	doi = {10.1016/j.anucene.2015.07.023},
	shorttitle = {Transient Fission Matrix},
	abstract = {In this paper, we present a time dependent version of the fission matrix method called Transient Fission Matrix ({TFM}) that we developed to perform accurate spatial kinetic calculations with a reduced computational cost. The matrices are generated once using the Monte Carlo neutron transport {MCNP} prior to the kinetic calculations. This article is devoted to the description of this approach with two different sets of equations depending on the aimed precision. We tested the {TFM} approach in several instances. In the calculation of the kinetic parameters such as βeff and Λeff on the Flattop and Jezebel experiments, the results obtained are compatible with adjoint weighted calculations performed with the Monte Carlo neutron transport code {SERPENT}. Likewise, our calculated value of the αRossi observable is also compatible with the experimental measurement. Finally a time–space prompt neutron propagation induced by a neutron pulse on Flattop has been calculated using the {TFM} approach and compared to a reference time dependent {SERPENT} calculation. Again, the results obtained demonstrate the good characterization of the time–space system response by the {TFM} approach.},
	pages = {1035--1044},
	journaltitle = {Annals of Nuclear Energy},
	shortjournal = {Annals of Nuclear Energy},
	author = {Laureau, A. and Aufiero, M. and Rubiolo, P. R. and Merle-Lucotte, E. and Heuer, D.},
	date = {2015},
	keywords = {Neutronics, Transient Fission Matrix, Kinetics, Transient},
	file = {Fulltext:/home/dotson/Zotero/storage/4JC699FZ/S0306454915003825.html:text/html;ScienceDirect Snapshot:/home/dotson/Zotero/storage/AXDDPT7V/S0306454915003825.html:text/html;Snapshot:/home/dotson/Zotero/storage/QHMMQAUH/S0306454915003825.html:text/html}
