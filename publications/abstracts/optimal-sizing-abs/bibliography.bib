@book{waleed_regional_2015,
	location = {University of Illinois at Urbana-Champaign},
	title = {Regional Deep Borehole Nuclear Fuel Repository: Requirements for a Site License Application},
	rights = {University of Illinois at Urbana-Champaign},
	abstract = {As of this writing it has been sixty years since the first nuclear electric power plant went online, but no country yet has an operational repository for nuclear fuel discharges. Although it is the country with the largest number of commercial nuclear electric power plants, as of this writing the United States had no program in progress to dispose of the nuclear fuel discharges it had already accumulated. The impediments to progress in this area are primarily political rather than technical. The Nordic Countries Sweden and Finland have both decided on sites for excavated repositories in granite. The U.S. Department of Energy has submitted a site license application for a repository excavated in volcanic tuff. An analysis from Sandia National Laboratory has suggests that a deep borehole repository is feasible that would exceed U.S. regulatory standards on migration of radioactive materials into groundwater by over a billion fold. As of early 2015, the U.S. Administration, the U.S. Congress, and the State of Nevada were nevertheless at an impasse on how to dispose of only part of the fuel that had already been discharged from commercial nuclear reactors.},
