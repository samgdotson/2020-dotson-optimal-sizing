	date = {2018-09-01},
	keywords = {Fuel cycle, Molten salt reactor, Spectral shift, Zirconium hydride},
	file = {ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/SZFV9Q2N/Betzler et al. - 2018 - Fuel cycle and neutronic performance of a spectral.pdf:application/pdf;ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/8MP5IAP8/Betzler et al. - 2018 - Fuel cycle and neutronic performance of a spectral.pdf:application/pdf;ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/T6DVVTNP/Betzler et al. - 2018 - Fuel cycle and neutronic performance of a spectral.pdf:application/pdf;ScienceDirect Snapshot:/home/dotson/Zotero/storage/CVPEBMPH/S0306454918302287.html:text/html;ScienceDirect Snapshot:/home/dotson/Zotero/storage/8H55YE7H/S0306454918302287.html:text/html;ScienceDirect Snapshot:/home/dotson/Zotero/storage/I298VS3S/S0306454918302287.html:text/html;Snapshot:/home/dotson/Zotero/storage/CPT9CPPE/S0306454918302287.html:text/html}
