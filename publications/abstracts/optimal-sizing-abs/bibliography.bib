	keywords = {small modular reactor, externality, molten-salt reactor, rare earth},
	file = {Full Text PDF:/home/dotson/Zotero/storage/Q54RA3YC/Kamei - 2012 - Recent Research of Thorium Molten-Salt Reactor fro.pdf:application/pdf;Snapshot:/home/dotson/Zotero/storage/AMBQBKSD/htm.html:text/html}
