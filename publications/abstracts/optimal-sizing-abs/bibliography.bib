	url = {https://www.sciencedirect.com/science/article/pii/S0306454917304735},
	doi = {10.1016/j.anucene.2017.12.022},
	abstract = {In the present work the utilization of thorium is investigated in a lead-cooled fast reactor. Two thorium fuel strategies were analyzed: a homogeneous and a heterogeneous mixture of 232Th were set in the European Lead-cooled Fast Reactor core model. For this, the Monte Carlo Serpent-2.27 code and the {JEFF}-3.1 cross section library were used to perform the calculations. Three neutronic parameters were analyzed for each fuel strategy: the effective neutron multiplication factor (keff) for a cycle of 900 days, the Doppler constant and the reactivity effect of coolant density. With the homogeneous mixture of 232Th the neutron multiplication factor increases during the whole operating cycle (900 days), allowing a longer fuel cycle, keeping negative values for the Doppler constant and the reactivity effect of coolant density, with −692 pcm and −148 pcm, respectively. On the other hand, the addition of 232Th heterogeneously in the core showed that there is the necessity to have a reactivity excess around 2.2\% Δk to fulfill the same fuel operating cycle. Even when there is a {ThO}2 blanket zone, the 233U production is not enough to increase the keff value and to enlarge the fuel operating cycle. Otherwise, the Doppler constant and the reactivity effect of coolant density are also negative values.},
	pages = {102--109},
	journaltitle = {Annals of Nuclear Energy},
	shortjournal = {Annals of Nuclear Energy},
	author = {Juárez-Martínez, Luis-Carlos and François, Juan-Luis},
	urldate = {2017-12-19},
	date = {2018-04},
	keywords = {Adiabatic reactor, Lead cooled fast reactor, Nuclear core design},
	file = {ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/LH552CWA/Juárez-Martínez and François - 2018 - Comparative neutronic study of homogeneous and het.pdf:application/pdf;ScienceDirect Snapshot:/home/dotson/Zotero/storage/3YQL8X8I/S0306454917304735.html:text/html}
}

