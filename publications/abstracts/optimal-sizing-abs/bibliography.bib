	type = {Submittal to Support {NRC} Development and Implementation of {DG}-1353, A Guidance to Risk-Inform Application Development and Contents Including Event Selection and {SSC} Classification},
	author = {{Oklo, Inc.}},
	date = {2018-09},
	file = {Oklo, Inc. - 2018 - Pilot Application for DG-1353, Enclosure 1.pdf:/home/dotson/Zotero/storage/ERRHRHKT/Oklo, Inc. - 2018 - Pilot Application for DG-1353, Enclosure 1.pdf:application/pdf}
}

