	keywords = {Experience curves, International comparison, Nuclear construction costs},
	file = {ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/T56G476G/Lovering et al. - 2016 - Historical construction costs of global nuclear po.pdf:application/pdf;ScienceDirect Snapshot:/home/dotson/Zotero/storage/V8WZZ2G3/S0301421516300106.html:text/html}
