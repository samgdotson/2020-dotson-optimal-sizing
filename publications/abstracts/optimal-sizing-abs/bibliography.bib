	keywords = {Hybrid energy systems, Nuclear power, Polygeneration systems},
	file = {ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/JQ3S73AI/Ruth et al. - 2014 - Nuclear-renewable hybrid energy systems Opportuni.pdf:application/pdf;ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/A6MLTJI6/Ruth et al. - 2014 - Nuclear-renewable hybrid energy systems Opportuni.pdf:application/pdf;ScienceDirect Snapshot:/home/dotson/Zotero/storage/HHC32SYC/S0196890413007516.html:text/html;ScienceDirect Snapshot:/home/dotson/Zotero/storage/JHAK53X9/S0196890413007516.html:text/html}
