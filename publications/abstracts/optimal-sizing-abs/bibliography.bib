@article{davis_comparison_2011,
	title = {Comparison of global variance reduction techniques for Monte Carlo radiation transport simulations of {ITER}},
	volume = {86},
	issn = {0920-3796},
	url = {http://www.sciencedirect.com/science/article/pii/S0920379611000718},
	doi = {10.1016/j.fusengdes.2011.01.059},
	series = {Proceedings of the 26th Symposium of Fusion Technology ({SOFT}-26)},
	abstract = {It is known that a priori information on the distribution of neutron fluxes around a neutron transport model can improve the computational efficiency of Monte Carlo transport calculations. The uncertainty lies in finding a method which obtains this information as fast and effortlessly as possible. A number of global variance reduction techniques were tested on the {ITER} reference neutronics model for their applicability for performing large three-dimensional transport simulations for {ITER}. Among these, we propose a new method which is computationally fast and efficient in determining the a priori flux information. The new method, known as {MAGIC}, was found to perform well achieving an efficiency 74 times that of an analogue run; the next most efficient was {FW}-{CADIS}, performing 48 times better than analogue.},
	pages = {2698--2700},
	number = {9},
	journaltitle = {Fusion Engineering and Design},
	shortjournal = {Fusion Engineering and Design},
	author = {Davis, Andrew and Turner, Andrew},
	date = {2011-10-01},
	keywords = {{MCNP}, Neutronics, {ITER}, Variance reduction},
	file = {[HTML] sciencedirect.com:/home/dotson/Zotero/storage/8D97DISU/S0920379611000718.html:text/html;ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/NA98697Z/Davis and Turner - 2011 - Comparison of global variance reduction techniques.pdf:application/pdf;ScienceDirect Snapshot:/home/dotson/Zotero/storage/GGN5UUBQ/S0920379611000718.html:text/html}
}

