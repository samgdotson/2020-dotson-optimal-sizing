@article{cervi_multiphysics_2019,
	title = {Multiphysics analysis of the {MSFR} helium bubbling system: A comparison between neutron diffusion, {SP}3 neutron transport and Monte Carlo approaches},
	volume = {132},
	issn = {0306-4549},
	url = {http://www.sciencedirect.com/science/article/pii/S0306454919302130},
	doi = {10.1016/j.anucene.2019.04.029},
	shorttitle = {Multiphysics analysis of the {MSFR} helium bubbling system},
	abstract = {The Molten Salt Fast Reactor is a fast-spectrum molten salt reactor under development in the framework of the European H2020 {SAMOFAR} Project (http://samofar.eu/). Among the design peculiarities, this circulating fuel reactor features a helium bubbling system aimed at removing on-line gaseous fission products, and metallic particles as well. From a modelling point of view, the presence of helium bubbles in the core needs to be assessed both from a neutronics and thermal-hydraulics point of view. In this paper, the attention is paid to the first aspect, analysing the void reactivity effect induced by the bubbles by means of a Monte Carlo, an {SP}3 neutron transport and a neutron diffusion approach. Since the distribution of helium bubbles is not uniform in the core but strongly depends on the fuel salt flow conditions, as well as on the location of their injection and extraction, a coupling scheme between Serpent 2 and {OpenFOAM} codes is adopted for the Monte Carlo analysis. In this way, the Monte Carlo code Serpent 2 is provided with a real bubble distribution calculated by means of a two-phase Euler-Euler solver implemented in {OpenFOAM}. The outcome puts in evidence the difference arising when a uniform and a real distribution of bubbles is considered in a Monte Carlo calculation. These results are also compared with an {SP}3 neutron transport and a neutron diffusion solver implemented in {OpenFOAM}, highlighting the difference among the three neutronics approaches herein adopted.},
	pages = {227--235},
	journaltitle = {Annals of Nuclear Energy},
	shortjournal = {Annals of Nuclear Energy},
	author = {Cervi, Eric and Lorenzi, Stefano and Luzzi, Lelio and Cammi, Antonio},
	urldate = {2019-05-06},
	date = {2019-10-01},
	langid = {english},
	keywords = {Molten Salt Reactor, Monte Carlo method, Multiphysics, Two-phase Euler-Euler solver},
	file = {ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/W27VNEFP/Cervi et al. - 2019 - Multiphysics analysis of the MSFR helium bubbling .pdf:application/pdf;ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/CBJPPHFD/Cervi et al. - 2019 - Multiphysics analysis of the MSFR helium bubbling .pdf:application/pdf;ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/74QBVAZ3/Cervi et al. - 2019 - Multiphysics analysis of the MSFR helium bubbling .pdf:application/pdf;ScienceDirect Snapshot:/home/dotson/Zotero/storage/5YJDIBN5/S0306454919302130.html:text/html;ScienceDirect Snapshot:/home/dotson/Zotero/storage/JCQI749H/S0306454919302130.html:text/html;ScienceDirect Snapshot:/home/dotson/Zotero/storage/2XX5NNE8/S0306454919302130.html:text/html}
}

@article{brown_k-effective_2011,
	title = {“K-effective of the World” and Other Concerns for Monte Carlo Eigenvalue Calculations},
	volume = {2},
	issn = {2185-4823},
	url = {http://www.aesj.or.jp/publication/pnst002/data/738-742.pdf},
	doi = {10.15669/pnst.2.738},
	abstract = {Monte Carlo methods have been used to compute keff and the fundamental mode eigenfunction of critical systems since the 1950s. Despite the sophistication of today’s Monte Carlo codes for representing realistic geometry and physics interactions, correct results can be obtained in criticality problems only if users pay attention to source convergence in the Monte Carlo iterations and to running a sufficient number of neutron histories to adequately sample all significant regions of the problem. Recommended best practices for criticality calculations are reviewed and applied to several practical problems for nuclear reactors and criticality safety, including the “K-effective of the World” problem. Numerical results illustrate the concerns about convergence and bias. The general conclusion of is that with today’s high-performance computers, improved understanding of the theory, new tools for diagnosing convergence (e.g., Shannon entropy of the fission distribution), and clear practical guidance for performing calculations, practitioners will have a greater degree of confidence than ever of obtaining correct results for Monte Carlo criticality calculations.},
	pages = {738--742},
	number = {0},
	journaltitle = {Progress in Nuclear Science and Technology},
	author = {Brown, Forrest B.},
	urldate = {2019-06-20},
