@article{lee_modeling_2016,
	title = {Modeling of oxide reduction in repeated-batch pyroprocessing},
	volume = {88},
	issn = {03064549},
	url = {http://linkinghub.elsevier.com/retrieve/pii/S0306454915005174},
	doi = {10.1016/j.anucene.2015.10.031},
	abstract = {Pyroprocessing is a complicated batch-type operation, involving a highly complex material flow logic with a huge number of unit processes. Discrete event system modeling was used to create an integrated operation model for which simulation showed that dynamic material flow could be accomplished to provide considerable insight into the process operation. In the model simulation, the amount of material transported upstream and downstream in the process satisfies a mass balance equation while considering the hold-up incurred by every batch operation. This study also simulated, in detail, an oxide reduction group process embracing electrolytic reduction, cathode processing, and salt purification. Based on the default operation scenario, it showed that complex material flows could be precisely simulated in terms of the mass balance. Specifically, the amount of high-heat elements remaining in the molten salt bath is analyzed to evaluate the operation scenario.},
	pages = {1--11},
	journaltitle = {Annals of Nuclear Energy},
	shortjournal = {Annals of Nuclear Energy},
	author = {Lee, Hyo and Suk Im, Hun and Il Park, Geun},
	date = {2016-02-01},
	langid = {english},
	file = {Lee et al. - 2016 - Modeling of oxide reduction in repeated-batch pyro.pdf:/home/dotson/Zotero/storage/CNLLDCTC/Lee et al. - 2016 - Modeling of oxide reduction in repeated-batch pyro.pdf:application/pdf;Lee et al. - 2016 - Modeling of oxide reduction in repeated-batch pyro.pdf:/home/dotson/Zotero/storage/ZWZVEMIU/Lee et al. - 2016 - Modeling of oxide reduction in repeated-batch pyro.pdf:application/pdf}
