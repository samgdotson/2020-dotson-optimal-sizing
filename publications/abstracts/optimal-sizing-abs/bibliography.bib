	author = {Scarlat, Raluca O. and Laufer, Michael R. and Blandford, Edward D. and Zweibaum, Nicolas and Krumwiede, David L. and Cisneros, Anselmo T. and Andreades, Charalampos and Forsberg, Charles W. and Greenspan, Ehud and Hu, Lin-Wen and Peterson, Per F.},
	urldate = {2017-05-11},
	date = {2014-11},
	keywords = {Licensing framework and reactor design, Liquid fluoride salt, Molten salt, Passive safety systems, reactor design},
	file = {ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/BEIT5HHS/Scarlat et al. - 2014 - Design and licensing strategies for the fluoride-s.pdf:application/pdf;ScienceDirect Snapshot:/home/dotson/Zotero/storage/429E8ZBF/S0149197014001838.html:text/html;ScienceDirect Snapshot:/home/dotson/Zotero/storage/HRWS95RI/S0149197014001838.html:text/html}
