	keywords = {Analytic Hierarchy Process, Hydrogen, Multi-criteria analysis, Natural gas, Power generation, Single-criterion analysis},
	file = {ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/VA49N8YS/Pilavachi et al. - 2009 - Multi-criteria evaluation of hydrogen and natural .pdf:application/pdf;ScienceDirect Snapshot:/home/dotson/Zotero/storage/AA5KHY6Q/S1359431108004523.html:text/html}
