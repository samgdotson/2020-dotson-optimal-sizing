	keywords = {{MSFR}, Neutronics, Thermal hydraulics, Transient calculation, Transient Fission Matrix},
	file = {ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/K5JUZTZ6/Laureau et al. - 2017 - Transient coupled calculations of the Molten Salt .pdf:application/pdf;ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/GEYY7UJ9/Laureau et al. - 2017 - Transient coupled calculations of the Molten Salt .pdf:application/pdf;ScienceDirect Snapshot:/home/dotson/Zotero/storage/IKKCKIDB/S002954931730081X.html:text/html;ScienceDirect Snapshot:/home/dotson/Zotero/storage/5B25JVQI/S002954931730081X.html:text/html}
