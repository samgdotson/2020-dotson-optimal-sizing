	keywords = {Burnup calculation, {CRAM}, Error analysis of {ORIGEN}-S, Molten Salt Reactor, Nonhomogeneous term, {TTA}},
	file = {Full Text:/home/dotson/Zotero/storage/KIB5H8F4/S0306454918305139.html:text/html;ScienceDirect Full Text PDF:/home/dotson/Zotero/storage/UFBH87EN/Xia et al. - 2019 - Development of a Molten Salt Reactor specific depl.pdf:application/pdf;ScienceDirect Snapshot:/home/dotson/Zotero/storage/N856EER7/S0306454918305139.html:text/html}
